# Example secrets file for Streamlit Cloud deployment
# Copy this to .streamlit/secrets.toml and fill in your actual values

# If you need API keys for future features (like Gemini integration)
# [api_keys]
# gemini_api_key = "your_gemini_api_key_here"

# Model configuration
[model]
model_path = "models/saved_models/binding_model.pt"
default_n_samples = 30
default_top_k = 20

# App configuration
[app]
title = "Synapse.AI - Drug Binding Prediction"
description = "AI-powered drug discovery platform for molecule-protein binding prediction"
